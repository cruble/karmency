<%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: {class: "form-horizontal"}, remote: true, format: :js) do |f| %>
    <%= devise_error_messages! %>
    <div class="form-group">
        <label for="first_name">First Name</label>
        <%= f.text_field :first_name, autofocus: true, class: "form-control" %>
    </div>
    <div class="form-group">
        <label for="last_name">Last Name</label>
        <%= f.text_field :last_name, class: "form-control" %>
    </div>

    <div class="form-group">
        <label for="exampleInputEmail11">Email address</label>
        <%= f.email_field :email, class: "form-control" %>
    </div>
    <div class="form-group">
        <label for="exampleInputPassword11">Password</label>

        <%= f.password_field :password, autocomplete: "off", class: "form-control", id: "exampleInputPassword11"  %>

    </div>
    <div class="form-group">
        <label for="exampleInputPassword111">Retype-Password</label>
        <%= f.password_field :password_confirmation, autocomplete: "off", class: "form-control" %>

    </div>
    <div class="pull-left checkbox">
        <!-- <label> -->
        <!--     <input type="checkbox"> Accept terms & condition. -->
        <!-- </label> -->

    </div>
    <div class="pull-right">
        <%= f.submit 'Register', class: "btn btn-theme-dark btn-md" %>
    </div>
<% end %>
<br>
<br>
<div class="clearfix"></div>
<div class="row social-login">
    <div class="col-sm-12">
        <div class="border-rits">
            <span>or</span>
        </div>
    </div>
    <div class="col-sm-12">
        <button class="btn btn-md btn-facebook"><i class="fa fa-facebook"></i> Register with facebook</button>
    </div>
</div>
